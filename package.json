{
  "name": "eslint-plugin-crc",
  "version": "0.1.3",
  "description": "Analyze and refactor JavaScript codebases with auto-generated Class-Responsibility-Collaboration models.",
  "keywords": [
    "refactoring",
    "refactor",
    "refactoring-tools",
    "javascript",
    "javascript-tools",
    "code-analysis",
    "cli",
    "ast",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": {
    "name": "Greg Swindle",
    "email": "greg@swindle.net",
    "url": "http://swindle.net"
  },
  "main": "lib/index.js",
  "scripts": {
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s",
    "docs": "npm run docs2html && npm run docs2md",
    "docs2html": "./node_modules/.bin/jsdoc -c jsdoc.conf.json",
    "docs2md": "./node_modules/.bin/jsdoc2md  --template ../eslint-plugin-crc.wiki/README.hbs --files ./lib/*.js > ../eslint-plugin-crc.wiki/API.md",
    "coverage": "./node_modules/.bin/nyc report",
    "coverage:all": "npm run coverage:codacy && npm run coverage:codecov && npm run coverage:coveralls",
    "coverage:codacy": "./node_modules/.bin/nyc report && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "coverage:codecov": "./node_modules/.bin/nyc report --reporter=text-lcov > ./coverage/coverage.lcov && ./node_modules/.bin/codecov `-t $CODECOV_TOKEN`",
    "coverage:coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "push": "git push origin develop && git checkout master && git merge develop && git push origin master && git checkout develop && git status",
    "security": "./node_modules/.bin/nsp check",
    "sonar": "sonar-scanner -Dproject.settings=./sonar-project.properties",
    "pretest": "eslint lib --fix && npm i snyk -g",
    "test": "./node_modules/.bin/snyk test && ./node_modules/.bin/nyc ./node_modules/.bin/mocha $(find tests -name '*.spec.js') --recursive",
    "posttest": "./node_modules/.bin/nsp check",
    "preversion": "npm test && ./node_modules/.bin/nsp check",
    "version": null,
    "postversion": "npm run changelog && node ./.ci-cd/bump/ && git commit --amend && git push && git push --tags",
    "prepublish": "./node_modules/.bin/snyk protect",
    "snyk": "./node_modules/.bin/snyk check",
    "snyk-protect": "./node_modules/.bin/snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregswindle/eslint-plugin-crc.git"
  },
  "bugs": {
    "url": "https://github.com/gregswindle/eslint-plugin-crc/issues"
  },
  "contributors": [],
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/fixtures/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "lines": 100,
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "babel-register": "^6.24.0",
    "eslint": "^3.18.0",
    "espree": "^3.4.0",
    "esrefactor": "^0.1.0",
    "estraverse": "^4.2.0",
    "global": "^4.3.1",
    "lodash": "^4.17.4",
    "minami": "^1.1.1",
    "relative-path": "^1.1.0",
    "replace": "^0.3.0",
    "require-dir": "^0.3.1",
    "requireindex": "~1.1.0",
    "snyk": "^1.25.2"
  },
  "devDependencies": {
    "bithound": "^1.7.0",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.1",
    "codeclimate-test-reporter": "^0.4.1",
    "codecov": "^2.1.0",
    "conventional-changelog-cli": "1.3.1",
    "coveralls": "^2.12.0",
    "dirty-chai": "^1.2.2",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.1.2",
    "nsp": "^2.6.3",
    "nyc": "^10.1.2",
    "properties-parser": "0.3.1"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT",
  "snyk": true
}
