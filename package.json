{
  "name": "eslint-plugin-crc",
  "version": "0.1.1",
  "description": "Analyze and refactor JavaScript codebases with auto-generated Class-Responsibility-Collaboration models.",
  "keywords": [
    "refactoring",
    "refactor",
    "refactoring-tools",
    "javascript",
    "javascript-tools",
    "code-analysis",
    "cli",
    "ast",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": {
    "name": "Greg Swindle",
    "email": "greg@swindle.net",
    "url": "http://swindle.net"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc mocha $(find tests -name '*.spec.js') --recursive",
    "coverage": "./node_modules/.bin/nyc report",
    "coverage:codacy": "./node_modules/.bin/nyc report && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "coverage:codecov": "./node_modules/.bin/nyc report --reporter=text-lcov > ./coverage/coverage.lcov && ./node_modules/.bin/codecov `-t $CODECOV_TOKEN`",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "docs": "rm -rf ./docs && ./node_modules/.bin/jsdoc -c jsdoc.conf.json",
    "push": "git push origin develop && git checkout master && git rebase develop && git push origin master && git checkout develop && git status",
    "sonar": "sonar-scanner -Dproject.settings=./sonar-project.properties"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregswindle/eslint-plugin-crc.git"
  },
  "bugs": {
    "url": "https://github.com/gregswindle/eslint-plugin-crc/issues"
  },
  "contributors": [],
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/fixtures/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "lines": 100
  },
  "dependencies": {
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.1",
    "codecov": "^1.0.1",
    "eslint": "^3.17.0",
    "espree": "^3.4.0",
    "esrefactor": "^0.1.0",
    "estraverse": "^4.2.0",
    "jsdoc": "^3.4.3",
    "lodash": "^4.17.4",
    "minami": "^1.1.1",
    "mocha-junit-reporter": "^1.13.0",
    "nyc": "^10.1.2",
    "relative-path": "^1.1.0",
    "require-dir": "^0.3.1",
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "mocha": "^3.1.2"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT"
}
