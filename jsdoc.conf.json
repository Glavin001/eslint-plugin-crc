{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["lib"],
    "exclude": ["tests"],
    "includePattern": "lib.+\\.js(doc|x)?$",
    "excludePattern": "tests(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
